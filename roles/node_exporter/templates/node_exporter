server {
	listen 80 default_server;
    root /var/www/html;
		location / {
		uwsgi_pass localhost:5000;
		include uwsgi_params;
		}
    location /metrics {
		proxy_pass http://localhost:9100/metrics;
	}
    location /prometheus {
        proxy_pass http://{{ groups['prometheus'][0]}}.{{domain}}:9090;
    }
}
